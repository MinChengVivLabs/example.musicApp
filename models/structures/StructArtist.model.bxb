structure (StructArtist) {
  description (struct of 'artist' table in db)
  property (artistID) {
    // id
    type (TypeArtistID)
    min (Required) max (One)
  }
  property (artistName) {
    // Name
    type (TypeArtistName)
    min (Required) max (One)
  }
  property (imageUrl) {
    // image url by JS assignment 
    type (TypeUrl)
    min (Required) max (One)
  }
}
